{
    "name": "cross-dom",
    "description": "A dom manipulation library, written in functional style.",
    "keywords": [
        "dom",
        "jquery",
        "fp",
        "functional"
    ],
    "version": "0.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "build": "babel src --out-dir lib",
        "test": "tape src/**/*.test.js -r esm -r ./browser-env | tap-summary",
        "test-w": "tape-watch src/**/*.test.js -r esm  -r ../browser-env | tap-summary",
        "test-browser": "airtap src/**/*.test.js --local"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "airtap": "^2.0.2",
        "babelify": "^10.0.0",
        "browser-env": "^3.2.6",
        "eslint": "^5.16.0",
        "eslint-config-byungi": "^0.2.9",
        "esm": "^3.2.22",
        "tap-summary": "^4.0.0",
        "tape": "^4.10.1",
        "tape-watch": "^2.3.0"
    }
}
